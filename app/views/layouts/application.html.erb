<!DOCTYPE html>
<html>
<head>
  <title>ThusastSrt</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if user_signed_in? %>
    <%= link_to 'sign_out', destroy_user_session_path, :method => :delete %>
  <% else %>
    <%= link_to 'sign_in', new_user_session_path %>
  <% end %>
<% if notice %>
  <p><%= notice %></p>
<% end %>
<% if alert %>
  <p><%= alert %></p>
<% end %>
<%= yield %>

</body>
</html>
